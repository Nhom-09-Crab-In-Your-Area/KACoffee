                                                    <option value="8">8</option>
                                                    <option value="9">9</option>
                                                    <option value="10">10</option>
                                                </select>
                                            </div>
                                            <div class="form-group col-xs-9 col-sm-9">
                                                <button id="add-to-cart" class="btn btn-primary btn-mg btn-block cart-add-event" type="submit" value="1">
                                                    <i class="material-icons" aria-hidden="true" tabindex="-1">shopping_cart</i>
                                                    Add to Cart
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-12 mini-description read-more-advanced">
